{
    "Logging": {
        "LogLevel": {
            "Default": "Warning",
            "Microsoft": "Warning",
            "Microsoft.Hosting.Lifetime": "Information",
            "gex": "Trace"
        }
    },
    "AllowedHosts": "*",
    "Discord": {
        "Enabled": false
    },
    "Services": {
        "LogoutBuffer": true
    },
    "Http": {
        "SearchEngineUserAgents": [
            "Yandex",
            "Googlebot",
            "Bingbot"
        ],
        "RealIpHeader":  "X-Connected-IP"
    }
}
